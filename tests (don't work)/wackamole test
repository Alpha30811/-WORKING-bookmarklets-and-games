(function () {
	var elem = document.createElement('div');
	var body = document.getElementsByTagName('body')[0];
	body.appendChild(elem);

	elem.style.position = 'fixed';
	elem.style.top = '0px';
	elem.style.left = '0px';
	elem.style.margin = '10px';
	elem.style.paddingTop = '10px';
	elem.style.width = '300px';
	elem.style.height = '300px';
	elem.style.zIndex = 10000;
	elem.style.opacity = 1;
	elem.style.color = 'green';
	elem.style.backgroundColor = 'green';
	elem.style.border = '0px solid white';
	elem.style.textAlign = 'center';
	elem.style.cursor = 'pointer';
	elem.id = 'myTimer';
	elem.style.display = 'block';
}());

function mole(x,y){
(function () {
	var elem = document.createElement('div');
	var body = document.getElementsByTagName('body')[0];
	body.appendChild(elem);

	elem.style.position = 'fixed';
	elem.style.top = ''+y+'px';
	elem.style.left = ''+x+'px';
	elem.style.margin = '10px';
	elem.style.paddingTop = '10px';
	elem.style.width = '40px';
	elem.style.height = '40px';
	elem.style.zIndex = 10000;
	elem.style.opacity = 1;
	elem.style.color = 'brown';
	elem.style.backgroundColor = 'brown';
	elem.style.border = '0px solid white';
	elem.style.textAlign = 'center';
	elem.style.cursor = 'pointer';
	elem.id = 'myTimer';
	elem.style.display = 'block';
}());
}
function makecoin(){
    var rx=Math.floor(Math.random()*(300-0+1)+0);
    var ry=Math.floor(Math.random()*(300-0+1)+0);
    mole(rx,ry);
}
setInterval(function() {
    setInterval(makecoin,Math.floor(Math.random()*(1000-500+1)+500));
},1000);
